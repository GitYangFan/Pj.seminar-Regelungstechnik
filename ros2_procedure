# play back ros bag
source /opt/ros/humble/setup.bash
ros2 bag play Hamster/rosbag/rosbag2_2023_03_01-16_48_29_0.db3

# run a python script
source /opt/ros/humble/setup.bash
cd Hamster/scripts
python3 <filename>

